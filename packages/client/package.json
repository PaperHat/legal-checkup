{
  "name": "legal-checkup-client",
  "version": "0.1.0",
  "private": true,
  "description": "The public client for the Legal Checkup application.",
  "author": "Charles F. Munat <chas@munat.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "apollo-cache-inmemory": "^1.3.7",
    "apollo-client": "^2.4.4",
    "apollo-link": "^1.2.3",
    "apollo-link-context": "^1.0.9",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "^1.5.5",
    "connected-react-router": "^4.5.0",
    "date-fns": "^2.0.0-alpha.23",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "history": "^4.7.2",
    "html-react-parser": "^0.4.7",
    "local-storage-fallback": "^4.1.1",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "ramda-adjunct": "^2.10.0",
    "react": "^16.6.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.6.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.1.0",
    "react-responsive": "^5.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.0.5",
    "react-select": "^2.1.1",
    "react-tag-autocomplete": "^5.6.0",
    "react-tooltip": "^3.9.0",
    "react-transition-group": "^2.5.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-observable": "^1.0.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "styled-components": "^4.0.2",
    "styled-map": "^3.2.0-rc.1",
    "styled-normalize": "^8.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/code-frame": "^7.0.0",
    "@babel/core": "^7.1.2",
    "@babel/parser": "^7.1.3",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^23.6.0",
    "babylon": "^6.18.0",
    "chalk": "^2.4.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.2",
    "import-sort": "^5.2.0",
    "import-sort-style-airkro": "^0.1.5",
    "istanbul-reports": "^2.0.1",
    "jest-enzyme": "^7.0.0",
    "jest-styled-components": "^6.2.2",
    "lint-staged": "^7.3.0",
    "node-fs-extra": "^0.8.2",
    "prettier-standard": "^8.0.1",
    "react-app-rewire-eslint": "^0.2.3",
    "react-app-rewire-inline-import-graphql-ast": "^1.1.1",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-app-rewired": "^1.6.2",
    "react-test-renderer": "^16.6.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-mock-store": "^1.5.3",
    "regenerator-runtime": "^0.12.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "!<rootDir>/node_modules/",
      "!src/constants.js",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!src/state/constants.js",
      "!src/utilities/constants.js",
      "src/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "scripts": {
    "build": "react-app-rewired build",
    "coverage": "react-app-rewired test --env=jsdom --coverage",
    "format": "prettier-standard 'src/**/*.js'",
    "precommit": "lint-staged",
    "start": "react-app-rewired start",
    "test": "react-app-rewired test --env=jsdom"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "airkro",
      "options": {}
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module",
      "options": {}
    }
  }
}
