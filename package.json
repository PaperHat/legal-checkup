  },
  "devDependencies": {
    "@babel/code-frame": "^7.0.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "chalk": "^2.4.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "husky": "^1.0.0-rc.15",
    "istanbul-reports": "^2.0.0",
    "jest-enzyme": "^6.1.2",
    "jest-styled-components": "^6.2.0",
    "lint-staged": "^7.3.0",
    "node-fs-extra": "^0.8.2",
    "prettier-standard": "^8.0.1",
    "react-app-rewire-aliases": "^0.2.0",
    "react-app-rewire-eslint": "^0.2.3",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-app-rewired": "^2.0.0",
    "react-scripts": "^2.0.0-next.2150693d",
    "react-test-renderer": "^16.5.2",
    "redux-devtools-extension": "^2.13.5",
    "redux-mock-store": "^1.5.3",
    "regenerator-runtime": "^0.12.1"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/components/index.js",
      "!src/components/styled/index.js",
      "!src/constants.js",
      "!src/containers/index.js",
      "!src/index.js",
      "!src/pages/index.js",
      "!src/registerServiceWorker.js",
      "!src/state/constants.js",
      "!src/state/epics/index.js",
      "!src/state/epics/rerouteOnThirdTest/index.js",
      "!src/state/index.js",
      "!src/utilities/index.js",
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "@components/(.*)$": "<rootDir>/src/components/$1",
      "@containers/(.*)$": "<rootDir>/src/containers/$1",
      "@pages/(.*)$": "<rootDir>/src/pages/$1",
      "@responsive/(.*)$": "<rootDir>/src/responsive/$1",
      "@state/(.*)$": "<rootDir>/src/state/$1",
      "@utilities/(.*)$": "<rootDir>/src/utilities/$1",
      "@wrappers/(.*)$": "<rootDir>/src/wrappers/$1"
    }
  },
  "scripts": {
    "build": "react-app-rewired build",
    "coverage": "react-app-rewired test --env=jsdom --coverage",
    "eject": "react-app-rewired eject",
    "start": "react-app-rewired start",
    "test": "react-app-rewired test --env=jsdom"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
